$(eval $(call addlib_s,libukprint,$(CONFIG_LIBUKPRINT)))

CINCLUDES-$(CONFIG_LIBUKPRINT)   += -I$(LIBUKPRINT_BASE)/include
CXXINCLUDES-$(CONFIG_LIBUKPRINT) += -I$(LIBUKPRINT_BASE)/include

LIBUKPRINT_CFLAGS-y   += -D__IN_LIBUKPRINT__
LIBUKPRINT_CXXFLAGS-y += -D__IN_LIBUKPRINT__

LIBUKPRINT_SRCS-y += $(LIBUKPRINT_BASE)/print.c
LIBUKPRINT_SRCS-$(CONFIG_HAVE_LIBC) += $(LIBUKPRINT_BASE)/snprintf.c
LIBUKPRINT_SRCS-y += $(LIBUKPRINT_BASE)/outf.c
LIBUKPRINT_SRCS-y += $(LIBUKPRINT_BASE)/hexdump.c
